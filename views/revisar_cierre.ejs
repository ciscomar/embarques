<% include ./static/header %>
<% include ./static/sidebar %>
<% include ./static/navbar %>

<div class="col-md-6 col-lg-10-2">
    <!-- general form elements -->
    <div class="box box-primary">
        <div class="box-header with-border">
            <br>
            <h3 class="box-title">Cierre Embarque #<%= embarque%></h3>
            <br>
        </div>
        <!-- /.box-header -->
        <!-- form start -->
        <form action="/guardar_equipo" method="POST" enctype="multipart/form-data">
            <div class="box-body">
                <!------->

                <div class="row">
                    <div class="form-group col-lg-6">
                        <label for="Titulo">Numero de Caja: </label><label id="caja"></label> <label id="msg">
                        </label>
                        <input type="text" class="form-control" id="caja" name="caja" value="<%= data[0].caja%>" readonly>
                    </div> 
      
                </div>

                    <div class="form-group">
                        <label for="Descripcion">Comentario:</label>
                        <textarea type="text" class="form-control " rows="3" name="comentario" readonly><%= data[0].comentario%></textarea>
                    </div>


                    <img class="d-block w-100" src="/uploads/<%= embarque%>.jpg">
                <!------->

        </form>
    </div>
    <!-- /.box -->

    <% include ./static/footer %>
</div>
</body>

</html>
