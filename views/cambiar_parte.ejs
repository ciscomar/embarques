<% include ./static/header %>
<% include ./static/sidebar %>
<% include ./static/navbar %>

<div class="col-md-6 col-lg-10-2">
    <!-- general form elements -->
    <div class="box box-primary">
        <div class="box-header with-border">
            <br>
            <h3 class="box-title">Programar Embarque</h3>
        </div>
        <!-- /.box-header -->
        <!-- form start -->
        <form action="/guardar_equipo" method="POST">
            <div class="box-body">
                <!------->
                <!-- Date format yyyy-m-d -->
 
                <!------->
                <div class="row">
                    <div class="form-group col-lg-6">
                        <label for="Titulo">Numero Embarque: </label><label id="embarque"></label> <label id="msg">
                        </label>
                        <input type="text" class="form-control" id="embarque" name="embarque" value="<%= data.idembarque%>" readonly>
                    </div>

                    <div class="form-group col-lg-6">
                        <label for="Titulo">Numero Parte: </label><label id="parte"></label> <label id="msg">
                        </label>
                        <input type="text" class="form-control" id="part" name="parte" value="<%= data.idparte%>" readonly>
                    </div>

      
                </div>

                <div class="row">


                    <div class="form-group col-lg-6">
                        <label for="Titulo">Cantidad: </label><label id="cantidad"></label> <label id="msg">
                        </label>
                        <input type="number" class="form-control" id="cantidad" name="cantidad" value="<%= data.cant%>" required>
                    </div>

                    <div class="form-group col-lg-6">
                        <label for="Titulo">Destino: </label><label id="destino"></label> <label id="msg">
                        </label>
                        <input type="text" class="form-control" id="destino" name="destino" value="<%= data.destino%>" required>
                    </div>
                </div>

                <!------->

                <div class="box-footer">
                    <button id="btnguardar" type="submit" class="btn btn-primary"
                        onclick="$('form').attr('action', '/guardar_cambio');">Guardar</button>
                </div>
        </form>
    </div>
    <!-- /.box -->

    <% include ./static/footer %>
</div>
</body>

</html>

